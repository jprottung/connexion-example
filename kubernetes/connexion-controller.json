{
  "kind": "ReplicationController",
  "apiVersion": "v1",
  "metadata": {
    "name": "connexion",
    "labels": {
      "app": "connexion"
    }
  },
  "spec": {
    "replicas": 3,
    "selector": {
      "app": "connexion"
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "connexion"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "connexion",
            "image": "jprottung/udacity-capstone-connexion:latest",
            "ports": [
              {
                "name": "http-server",
                "containerPort": 8080
              }
            ]
          }
        ]
      }
    }
  }
}
